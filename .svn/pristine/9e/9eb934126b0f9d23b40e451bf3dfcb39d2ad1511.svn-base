package de.fhe.data;

import java.io.Serializable;

/**
 * @author Corlan McDonald
 * 
 * This class is the container of the "Channel" tag under the RSS Feed...
 */

public class MoviePreviewsRSSFeed implements Serializable
{
	private static final long serialVersionUID = -5309506665212618876L;
	
	private String title;

	private MoviePreviewsList moviePreviewsList;

	public MoviePreviewsRSSFeed ()
	{
		setTitle(null);
		setItems(null);
	}

	public String getTitle()
	{
		return title;
	}

	public void setTitle(String title)
	{
		this.title = title;
	}

	public MoviePreviewsList getItems()
	{
		return moviePreviewsList;
	}

	public void setItems(MoviePreviewsList moviePreviewsList)
	{
		this.moviePreviewsList = moviePreviewsList;
	}
}
